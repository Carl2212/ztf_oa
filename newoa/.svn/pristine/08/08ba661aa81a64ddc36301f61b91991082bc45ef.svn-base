.important-bg-primary {
	color: #fff!important;
	background-color: #337ab7!important
}

a.important-bg-primary:hover {
	background-color: #286090!important
}

.list-group-item:hover{
	background-color:#dfdfdf
}

.detail-table-label{
	min-width:75px;
	max-width:125px;
	font-weight:bold;
	vertical-align: middle;
}

/* *{
	background-color: transparent!important;
} */

body{
	overflow-x: hidden;
	position: relative;
}

.panel-primary>.panel-heading {
 background-color: rgba(53, 123, 184,.8)!important;
}

.panel-default>.panel-heading {
background-color: rgba(230, 230, 230,.8)!important;
}

.list-group-item {
	background-color: transparent!important;
}

.panel {
	background-color: transparent!important;
}

.table-striped>tbody>tr:nth-of-type(odd) {
	background-color: rgba(252, 252, 252,.5)!important;
}

.cdc-bg{
	  filter:alpha(opacity=30);  
      -moz-opacity:0.3;  
      -khtml-opacity: 0.3;  
      opacity: 0.4;  
      position: absolute;
      top:0;
	  overflow: hidden;
	  color: #333;
	  word-spacing: 30px;
      font-size: 12px;
	  z-index: -999;
	  height:100%
}

.cdc-bg div{
	padding:50px 0;
	padding-left: 50px;
	-webkit-transform: rotate(-40deg);
	-moz-transform: rotate(-40deg);
	float:left
}

.cdc-bg img{
	height: 30px;
	position: absolute;
	left: 0;
	top: 50%;
	margin-top: -15px;
}

.ico_radio_check{
	width:20px;
}

.float-right{
	position: absolute;
	top: 50%;
	right: 15px;
	margin-top: -17px;
}

textarea
{
	width:100%;
}

.cdc-form-input .cdc-input-label{
	text-align:right;
	padding: 7px 0 7px;
}

.cdc-scroll-panel-body{
	overflow-y: scroll;
}