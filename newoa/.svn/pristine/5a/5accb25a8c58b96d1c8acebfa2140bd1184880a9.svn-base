<div class="modal fade" id="myModal" tabindex="-1" role="dialog" 
   aria-labelledby="myModalLabel" aria-hidden="true">
   <div class="modal-dialog">
      <div class="modal-content">
         <div class="modal-header">
            <button type="button" class="close" 
               data-dismiss="modal" aria-hidden="true">
                  &times;
            </button>
            <h4 class="modal-title" id="myModalLabel">
                	成员信息 
            </h4>
         </div>
         <div class="modal-body">
               <table class="table table-striped" style="max-width:100%">
				   
				   <tbody id="detailDom">
				      <tr>
				         <td class="detail-table-label" style="width:30%">姓名</td>
				         <td id="cnname">Bangalore</td>
				      </tr>
				      <tr>
				         <td class="detail-table-label">部门</td>
				         <td id="groupname"></td>
				      </tr>
				      <tr>
				         <td class="detail-table-label">办公电话</td>
				         <td id="officephone"></td>
				      </tr>
				      <tr>
				         <td class="detail-table-label">手机</td>
				         <td id="mobile">
				         	
				         	<a href="#">
					          <span class="glyphicon glyphicon-glyphicon-phone"></span>
					        </a>
				         	<a href="#">
					          <span class="glyphicon glyphicon-envelope"></span>
					        </a>
				         </td>
				      </tr>
				      <tr>
				         <td class="detail-table-label">邮箱</td>
				         <td id="email"></td>
				      </tr>
				      
				   </tbody>
				</table>
         </div>
         <div class="modal-footer">
            <button type="button" class="btn btn-primary" 
               data-dismiss="modal">关闭
            </button>
         </div>
